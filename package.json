{
  "name": "pagefy-api",
  "version": "1.19.0",
  "description": "Pagefy API",
  "main": "src/server.js",
  "scripts": {
    "start": "env-cmd -f ./.env.dev npx tsx src/server.ts",
    "start:dev": "npm run swagger-autogen && npm run update-pkg-version && env-cmd -f ./.env.dev nodemon -x ts-node src/server.ts",
    "swagger-autogen": "ts-node ./src/config/swagger.ts",
    "update-pkg-version": "ts-node ./src/config/package-json.ts && prettier --write ./package.json",
    "deploy-main": "git checkout main && git merge develop && git push origin main && git checkout develop",
    "build": "webpack"
  },
  "keywords": [],
  "author": "Marcelino Borges",
  "engines": {
    "node": "20.x",
    "npm": "10.x"
  },
  "license": "ISC",
  "dependencies": {
    "@google-cloud/storage": "^6.5.2",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "axios": "^0.25.0",
    "cors": "^2.8.5",
    "env-cmd": "^10.1.0",
    "express": "^4.17.1",
    "firebase-admin": "^10.0.2",
    "helmet": "^4.6.0",
    "moment": "^2.29.1",
    "mongoose": "^5.13.7",
    "multer": "^1.4.4",
    "nodemailer": "^6.8.0",
    "nodemon": "^2.0.12",
    "save": "^2.4.0",
    "swagger-ui-express": "^4.1.6",
    "ts-node": "^10.2.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/dotenv-safe": "^8.1.2",
    "@types/express": "^4.17.13",
    "@types/helmet": "^4.0.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.6.0",
    "@types/nodemailer": "^6.4.6",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.8.0",
    "dotenv-safe": "^8.2.0",
    "prettier": "^3.5.2",
    "swagger-autogen": "^2.22.0",
    "ts-loader": "^9.5.2",
    "typescript": "^4.3.5",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1"
  }
}
