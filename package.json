{
  "name": "socialbio-api",
  "version": "1.4.0",
  "description": "SocialBio API TESTE",
  "main": "src/server.js",
  "scripts": {
    "build": "npm install",
    "start": "env-cmd -f ./.env nodemon -x ts-node src/server.ts",
    "start_dev": "npm run swagger-autogen && npm run update-pkg-version && env-cmd -f ./.env nodemon -x ts-node src/server.ts",
    "swagger-autogen": "ts-node ./src/config/swagger.ts",
    "update-pkg-version": "ts-node ./src/config/package-json.ts",
    "push-main": "git checkout main && git merge develop && git push origin main"
  },
  "keywords": [],
  "author": "Marcelino Borges",
  "engines": {
    "node": "16.x",
    "npm": "8.x"
  },
  "license": "ISC",
  "dependencies": {
    "axios": "^0.25.0",
    "cors": "^2.8.5",
    "env-cmd": "^10.1.0",
    "express": "^4.17.1",
    "firebase-admin": "^10.0.2",
    "helmet": "^4.6.0",
    "moment": "^2.29.1",
    "mongoose": "^5.13.7",
    "multer": "^1.4.4",
    "nodemailer": "^6.8.0",
    "nodemon": "^2.0.12",
    "save": "^2.4.0",
    "swagger-ui-express": "^4.1.6",
    "ts-node": "^10.2.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
